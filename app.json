{
  "expo": {
    "name": "QuietNote",
    "slug": "quiet-note",
    "description": "A private, offline journal appfor reflection, mood tracking, and idea capture.",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "quietnote",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.tawwar.quietnote"
    },
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      // "imageWidth": 200,
      "backgroundColor": "#F8FAFC",
      "dark": {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain",
        // "imageWidth": 200,
        "backgroundColor": "#0F172A"
      }
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "41fdb0f1-5246-485a-9545-d9bcbdf804c0"
      }
    }
  }
}